{
    "out": "./docs/",
    "name": "blockly-local-variable",
    "entryPoints": ["./src"],
    "exclude": [
        "node_modules/**/*",
        "dist/**/*",
        "lib/**/*",
        "esm/**/*",
        "docs/**/*"
    ],
    "externalPattern": [
        "node_modules/**/*",
        "dist/**/*",
        "lib/**/*",
        "esm/**/*",
        "docs/**/*"
    ],
    "excludeExternals": true,
    "excludeInternal": false,
    "excludePrivate": true,
    "hideGenerator": true,
    "plugin": "none",
    "readme": "./README.md"
}